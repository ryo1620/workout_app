<% number = 0 %>
<% day_menus.each do |day_menu| %>
  <% number += 1 %>
  <%= fields_for 'week_menus[]', day_menu do |field| %>
    <%=
      field.collection_select :menu_id,
      @user.menus,
      :id,
      :name,
      {:include_blank => true},
      {:class => "custom-select", :id => "week_menus#{count}_#{number}"}
    %>
  <% end %>
<% end %>